{
  "@sakamoto": {
    "retention": {
      "keepHourly": 24,
      "keepDaily": 7
    },
    "compression": {
      "compressorName": "zstd-fastest"
    }
  },
  "@vps": {
    "retention": {
      "keepHourly": 24,
      "keepDaily": 7
    },
    "compression": {
      "compressorName": "zstd-fastest"
    }
  },
  "shelken@sakamoto:/backup/minio": {
    "scheduling": { "intervalSeconds": 3600 }
  },
  "shelken@sakamoto:/backup/compose": {
    "scheduling": { "intervalSeconds": 3600 }
  },
  "shelken@vps:/backup/data": {
    "scheduling": { "intervalSeconds": 3600 }
  }
}
